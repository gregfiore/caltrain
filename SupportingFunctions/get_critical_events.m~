function critical_incidents = get_critical_events()

critical_incidents{1,1} = {'fatal',;...
                           'killed',;...
                           'suicide';...
                           'incident',;...
                           'accident','struck'};
critical_incidents{3,1} = {'indefinitely','cancel','severe','broke', 'mechanical'};
critical_incidents{4,1} = {'restrict','signal'};
